<!DOCTYPE html>
<html>
  <head>
    <h1>Exercise Tracker</h1>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div class="login">
    <% if !current_user %>
        <%= link_to "Sign in with Google", "/auth/google_oauth2", id: "sign_in" %>
    <% end %>
    </div>
    
    <div>
      <div id="flash_error">
        <% if flash[:error] %>
          <%= flash[:error] %>
        <% end %>
      </div>
    </div>
    
    <div class="container">
      Project by Team Girl Scout Cookies
      <div class="span-24">
      </div>
    </div>
  </body>
</html>